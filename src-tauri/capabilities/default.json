{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions + FS & Dialog",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "process:default",
    {
      "identifier": "fs:scope",
      "allow": [{ "path": "$HOME/**/*" }]
    }
  ]
}
